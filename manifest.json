{
  "name": "Kronymous - Access internet via Tor Network",
  "short_name": "Kronymous",
  "description": "",
  "version": "0.2.6",
  "manifest_version": 2,
  "minimum_chrome_version": "36",
  "icons": {
    "16": "kronymous_16.png",
    "48": "kronymous_48.png",
    "128": "kronymous_128.png"
  },
  "offline_enabled": false,
  "permissions": [
    "clipboardWrite",
    "storage",
    "unlimitedStorage",
    {
      "socket": [
        "tcp-listen:*:*",
        "tcp-connect",
        "resolve-host"
      ]
    }
  ],
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  }
}
